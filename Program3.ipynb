        "import numpy as np\n",
        "\n",
        "X = np.array([[1, 2], [3, 4]])\n",
        "Y = np.array([[5, 6], [7, 8]])\n",
        "\n",
        "print(\"Matrix X is:\\n\", X)\n",
        "print(\"Matrix Y is:\\n\", Y)\n",
        "\n",
        "a = np.power(X, 2)\n",
        "print(\"X^2 =\\n\", a)\n",
        "\n",
        "result = a + 2 * Y\n",
        "print(\"X^2 + 2*Y is\\n\", result)\n"
